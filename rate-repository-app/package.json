{
  "name": "rate-repository-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "compile-gql": "graphql-codegen -r dotenv/config",
    "compile-gql:watch": "npm run compile-gql -- -w",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@expo/html-elements": "^0.3.0",
    "@expo/metro-config": "^0.5.1",
    "@expo/webpack-config": "^0.17.0",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@types/react": "~18.0.0",
    "@types/react-native": "~0.69.1",
    "dotenv": "^16.0.3",
    "expo": "~46.0.16",
    "expo-linking": "~3.2.4",
    "expo-status-bar": "~1.4.0",
    "formik": "^2.2.9",
    "graphql": "^15.8.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.6",
    "react-native-paper": "^5.1.3",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-web": "~0.18.7",
    "react-router-native": "^6.4.3",
    "typescript": "^4.6.3",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@graphql-codegen/cli": "^2.13.12",
    "@graphql-codegen/client-preset": "^1.1.5",
    "@graphql-eslint/eslint-plugin": "^3.13.1",
    "@testing-library/jest-native": "^5.3.3",
    "@testing-library/react-native": "^11.5.0",
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "cross-fetch": "^3.1.5",
    "eslint": "^8.27.0",
    "eslint-config-universe": "^11.1.1",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-react-native-a11y": "^3.3.0",
    "jest": "^27.5.1",
    "jest-expo": "^47.0.1",
    "msw": "^0.49.2",
    "prettier": "^2.7.1",
    "react-native-dotenv": "^3.4.2",
    "react-test-renderer": "^18.0.0"
  },
  "private": true,
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "./jest.setup.js"
    ],
    "setupFilesAfterEnv": [
      "./src/test/jest-after-env.setup.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|react-router-native)"
    ]
  }
}
